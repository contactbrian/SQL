/* Select all the columns and rows from the table in the database */

SELECT * FROM superbowls;


/* How many Superbowls has each team won? */

SELECT Winner, COUNT(Winner) AS Total_SB_Wins
FROM superbowls 
GROUP BY Winner
ORDER BY Winner asc; 



/* Which teams have won 3 or more Superbowls and how many have they won? */

SELECT Winner, COUNT(Winner) AS Total_SB_Wins
FROM superbowls 
GROUP BY Winner
HAVING Total_SB_Wins >=3 
ORDER BY Total_SB_Wins desc;




/* How many times has an AFC East Team (New England Patriots, New York Jets, Buffalo Bills, Miami Dolphins) represented the AFC in the Superbowl? */ 

SELECT COUNT (Winner AND Loser) AS AFC_East_SB_Appearances
FROM superbowls
WHERE Winner IN ("New England Patriots", "New York Jets", "Miami Dolphins", "Buffalo Bills") OR Loser IN ("New England Patriots", "New York Jets", "Miami Dolphins", "Buffalo Bills");




/* What is the average amount of points the winning team scored? */

SELECT AVG(Winner_Pts) FROM superbowls;




/* In what Superbowls did teams score more than 30 points in their win, and how many points did they score? */

SELECT Winner, Winner_Pts, SB
FROM superbowls
GROUP BY Winner
HAVING Winner_Pts > 30
ORDER BY Winner_Pts desc;  



/* What is the average point differential between the winning and losing teams? */

SELECT AVG (Winner_Pts - Loser_Pts) AS Avg_Point_Diff FROM superbowls;




/* How do the winning teams performance compare to the Average Point Differential? */

SELECT SB, Winner, AVG (Winner_Pts - Loser_Pts) AS "Point_Diff",
    CASE 
        WHEN AVG (Winner_Pts - Loser_Pts) >= 14 THEN "Above Average"
        ELSE "Below Average"
    END as "Rating"
FROM superbowls
GROUP BY SB
ORDER BY SB;
